<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="List danh sÃ¡ch!"
        app:layout_constraintBottom_toTopOf="@+id/trangthanhtoan"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.464"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.289" />

    <Button
        android:id="@+id/trangthanhtoan"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:text="Trang Thanh Toan"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toTopOf="@id/trangsetting"/>

    <Button
        android:id="@+id/trangsetting"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:text="Trang Setting"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintBottom_toTopOf="@id/trangscreening"
        app:layout_constraintTop_toBottomOf="@+id/trangthanhtoan" />

    <Button
        android:id="@+id/trangscreening"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:text="Trang screening"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintBottom_toTopOf="@id/trangperson"
        app:layout_constraintTop_toBottomOf="@+id/trangsetting" />

    <Button
        android:id="@+id/trangperson"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:text="Trang Person"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintBottom_toTopOf="@id/trangprofilephim"
        app:layout_constraintTop_toBottomOf="@+id/trangscreening" />

    <Button
        android:id="@+id/trangprofilephim"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:text="Trang Profile phim"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/trangperson" />

    <Button
        android:id="@+id/trangseat"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:text="Trang seat"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.502"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/trangscreening"
        app:layout_constraintVertical_bias="0.164" />

    <Button
        android:id="@+id/signup"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:text="Trang signup"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.502"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/trangscreening"
        app:layout_constraintVertical_bias="0.164" />
</androidx.constraintlayout.widget.ConstraintLayout>